{
  "name": "@outburn/structure-navigator",
  "version": "1.9.2",
  "description": "Navigate and resolve FHIR element definitions using FSH-like paths",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "author": "Outburn Ltd.",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && tsup",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "dist-to-module": "npm run build && rimraf node_modules/@outburn/structure-navigator && node test/dist-to-module.js",
    "pretest": "npm run dist-to-module",
    "test": "vitest run",
    "debug:build": "npm run dist-to-module && ncc build test/debug.ts --external fhir-snapshot-generator --no-cache --out test/dist/debug && node test/dist/debug/index.js",
    "debug": "npm run debug:build && node test/dist/debug/index.js"
  },
  "keywords": [
    "fhir",
    "hl7",
    "package",
    "profile",
    "conformance",
    "cache",
    "structuredefinition",
    "snapshot",
    "elementdefinition",
    "type",
    "structure",
    "element",
    "fsh",
    "path"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outburn-IL/fhir-structure-navigator.git"
  },
  "peerDependencies": {
    "@outburn/types": "^0.1.0 || ^0.2.0",
    "fhir-package-explorer": "^1.9.2",
    "fhir-snapshot-generator": "^2.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@outburn/types": "^0.2.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.2",
    "fhir-package-explorer": "^1.9.2",
    "fhir-snapshot-generator": "^2.2.2",
    "fs-extra": "^11.3.3",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "lru-cache": "^11.2.6"
  }
}
